<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.layout.VBox?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" spacing="5" stylesheets="@../css/dialog_box.css" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox fx:id="timeStampContainer" >
            <Label fx:id="timeStamp" text="timeStamp" wrapText="true" />
        </VBox>
            <VBox fx:id="nameAndDialog" xmlns:fx="http://javafx.com/fxml" >
                <Label fx:id="name" text="name" wrapText="true" />
                <Label fx:id="dialog" text="Label" maxWidth="200" wrapText="true" />
            </VBox>

        <Circle fx:id="displayPicture"  pickOnBounds="true" />
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
